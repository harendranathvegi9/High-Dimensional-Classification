np = 100;
md = 20;
M = zeros(1,md);

for d = 1:5:md
   pts =  rand(d,np);
    count = 0;
    total = 0;
    lengths = [];
   for i = 1:np
       for j = (i+1):np
           L = norm(pts(:,i)- pts(:,j));
           total = total + L;
           count = count+1;
           lengths = [lengths L];
       end
   end
   hist(lengths,linspace(0,5,15));
   pause;
   A(d) = total/count;
end
%plot(A);